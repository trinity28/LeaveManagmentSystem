 <h2 class="page-header">List of all leaves</h2> 
<p>Welcome to your dashboard</p> 
{{#each contact}}

    <div class="row"></div>
    <table>
      <tr>
        <th>Requested_by</th>
         <th>Requested_At </th>
        <th>Start_date</th>
        <th>End_Date</th>
        <th>Leave_Type</th>
        <th>Status</th>
         <th>Response</th>
          
      </tr>
     <tr>
     <td>{{this.RequestBy}}</td>
       <td>{{this.RequestedAt}}</td>
        <td>{{this.StartDate}}</td>
        <td>{{this.EndDate}}</td>
        <td>{{this.LeaveType}}</td>
        <td>{{this.ApprovalStatus}}</td>
        
      
      

           
              <td> <form method="post" action="/leaves/{{this._id}}?_method=PUT">
              <input type="hidden" name="_method" value="put">
               <div class="form-group">
              <select name="ApprovalStatus">
                <option value="Approved">Approved</option>
                <option value="Rejected">Rejected</option>   
              </select>
          </div>
           <button type="submit" class="btn btn-default">Submit</button>
    </form></td>
          
       
       
  </tr>
    </table>
   
{{/each}}